<div class="text-center">
    <h3>Please confirm the following information</h3>
    <hr>
    <div class="row">
    	<div class="col-lg-6">
    		<ul class="confirm-list">
    			<li><label>Name:</label>{{formData.name}}</li>
    			<li><label>Company:</label>{{formData.company}}</li>
    			<li><label>Email:</label>{{formData.email}}</li>
    			<li><label>Phone #:</label>{{formData.phoneno}}</li>
    		</ul>
    	</div>
    	<div class="col-lg-6">
    		<ul class="confirm-list">
		    	<li><label>Address:</label>{{formData.address}}</li>
		    	<li><label>State:</label>{{formData.state}}</li>
		    	<li><label>City:</label>{{formData.city}}</li>
		    	<li><label>Zipcode:</label>{{formData.zipcode}}</li>
	    	</ul>
    	</div>
    </div>
    <div class="row">
    	<div class="col-lg-12">
			<ul class="confirm-list">
				<li><label>Class:</label>{{formData.class}}</li>
			</ul>
    	</div>
    </div>
    <hr>
    
    
    <button type="submit" class="btn btn-danger">Submit Registration</button>
</div>